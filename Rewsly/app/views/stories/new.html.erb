
<h2>Enter a new Story</h2>

    <%= form_for(@stories) do |f| %>
      <%= f.label :title %>
      <%= f.text_field :title %></br>

      <%= f.label :link %>
      <%= f.email_field :link %></br>

      <%= f.label :upvotes %>
      <%= f.text_field :upvotes %></br> <!--possible issue with integration because returns string?-->

      <%= f.label :category %>
      <%= f.text_field :category %></br>

      <%= f.submit "Create a New Story" %>
    <% end  %>
    
<h2>Find a Current Story!</h2>

<%= form_tag('stories_search_path', :method => "get") do %> #figure out search
  <%= label_tag(:title, "Search for:") %>
  <%= text_field_tag(:title) %>
  <%= submit_tag("Search") %>
  
<% end %>

<p>enters</p>
















